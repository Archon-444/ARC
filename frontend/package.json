    "@circle-fin/smart-contract-platform": "^9.3.0",
    "@circle-fin/user-controlled-wallets": "^9.3.0",
    "@circle-fin/w3s-pw-web-sdk": "^1.1.11",
    "@rainbow-me/rainbowkit": "^2.2.9",
    "@tanstack/react-query": "^5.90.10",
    "algoliasearch": "^4.25.3",
    "axios": "^1.13.2",
    "clsx": "^2.1.0",
    "date-fns": "^3.0.0",
    "framer-motion": "^12.23.24",
    "graphql": "^16.12.0",
    "graphql-request": "^6.1.0",
    "lucide-react": "^0.554.0",
    "next": "16.0.7",
    "next-auth": "^4.24.13",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hotkeys-hook": "^5.2.1",
    "react-instantsearch": "^7.20.0",
    "react-use": "^17.6.0",
    "react-virtuoso": "^4.14.1",
    "recharts": "^3.4.1",
    "typescript": "^5.9.3",
    "typesense": "^2.1.0",
    "urql": "^5.0.1",
    "viem": "^2.39.2",
    "wagmi": "^2.19.4",
    "web-vitals": "^5.1.0"
