{
  "name": "arcmarket-frontend",
  "version": "0.2.0",
  "description": "ArcMarket - NFT Marketplace on Circle Arc Blockchain",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "analyze": "ANALYZE=true next build",
    "clean": "rm -rf .next node_modules"
  },
  "keywords": ["nft", "marketplace", "arc", "circle", "usdc", "blockchain"],
  "author": "ArcMarket Team",
  "license": "MIT",
  "dependencies": {
    "@circle-fin/developer-controlled-wallets": "^9.3.0",
    "@circle-fin/user-controlled-wallets": "^9.3.0",
    "@rainbow-me/rainbowkit": "^2.2.9",
    "@tanstack/react-query": "^5.90.10",
    "@types/react": "^19.2.6",
    "axios": "^1.13.2",
    "lucide-react": "^0.554.0",
    "next": "^16.0.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "^5.9.3",
    "viem": "^2.39.2",
    "wagmi": "^2.19.4",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^8.56.0",
    "eslint-config-next": "^16.0.3",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.2.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^4.1.17",
    "jest": "^29.7.0",
    "@testing-library/react": "^14.2.0",
    "@testing-library/jest-dom": "^6.2.0",
    "jest-environment-jsdom": "^29.7.0"
  }
